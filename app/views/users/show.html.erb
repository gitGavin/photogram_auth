<div class="page-header">
  <h3>User #<%= @user.username %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <table class="table table-striped table-hover">
      <% @user.photos.each do |photo| %>
        <dl class="dl-horizontal">
          <dt>Caption</dt>
          <dd><%= photo.caption %></dd>

          <dt>Image</dt>
          <dd><%= photo.image %></dd>
        </dl>
      <% end %>
    </table>
  </div>
</div>
